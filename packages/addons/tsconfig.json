{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es6",
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "extends": "../../tsconfig.json",
  "include": [
    "./**/*"
  ],
  "exclude": [
    "./dist/*"
  ]
}
